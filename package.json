{
  "name": "ai-frontend-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rsbuild build",
    "dev": "rsbuild dev",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "mock": "json-server --watch mock/db.json --routes mock/routes.json --port 3001",
    "prettier": "prettier -c --write . --cache",
    "prettier-import-sort": "npm run prettier -- --plugin=@ianvs/prettier-plugin-sort-imports",
    "preview": "rsbuild preview",
    "type-check": "tsc --noEmit",
    "update-context": "node scripts/update-context.cjs"
  },
  "dependencies": {
    "@ant-design/icons": "^5.4.0",
    "@ant-design/pro-components": "^2.8.0",
    "@dalydb/sdesign": "^1.1.5",
    "ahooks": "^3.8.0",
    "antd": "^5.20.0",
    "axios": "^1.7.0",
    "chart.js": "^4.5.1",
    "dayjs": "^1.11.0",
    "immer": "^10.1.0",
    "lodash-es": "^4.17.0",
    "lucide-react": "^0.400.0",
    "react": "^18.3.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^18.3.0",
    "react-router-dom": "^6.26.0",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.1",
    "@rsbuild/core": "^1.7.0",
    "@rsbuild/plugin-react": "^1.3.0",
    "@types/lodash-es": "^4.17.0",
    "@types/node": "^22.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "json-server": "^1.0.0-beta.0",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js"
    ]
  }
